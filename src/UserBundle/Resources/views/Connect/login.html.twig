{% extends "UserBundle::layout.html.twig" %}

{% block user_content %}
    {% if error is defined and error %}
        <span>{{ error }}</span>
    {% endif %}
    {% for owner in user_oauth_resource_owners() %}
    <a href="{{ user_oauth_login_url(owner) }}">{{ owner | trans({}, 'HWIOAuthBundle') }}</a> <br />
    {% endfor %}
{% endblock user_content %}
